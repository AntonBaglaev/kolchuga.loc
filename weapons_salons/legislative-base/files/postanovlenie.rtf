\b0\i0\par\pard\sa150\qj 72.\u32 \u1048\u1085\u1092\u1086\u1088\u1084\u1072\u1094\u1080\u1103\u32 \u1086\u32 \u1084\u1077\u1076\u1080\u1094\u1080\u1085\u1089\u1082\u1080\u1093\u32 \u1080\u1079\u1076\u1077\u1083\u1080\u1103\u1093\u32 (\u1080\u1085\u1089\u1090\u1088\u1091\u1084\u1077\u1085\u1090\u1072\u1093,\u32 \u1072\u1087\u1087\u1072\u1088\u1072\u1090\u1072\u1093,\u32 \u1087\u1088\u1080\u1073\u1086\u1088\u1072\u1093,\u32 \u1086\u1073\u1086\u1088\u1091\u1076\u1086\u1074\u1072\u1085\u1080\u1080,\u32 \u1084\u1072\u1090\u1077\u1088\u1080\u1072\u1083\u1072\u1093\u32 \u1080\u32 \u1087\u1088\u1086\u1095\u1080\u1093\u32 \u1080\u1079\u1076\u1077\u1083\u1080\u1103\u1093,\u32 \u1087\u1088\u1080\u1084\u1077\u1085\u1103\u1077\u1084\u1099\u1093\u32 \u1074\u32 \u1084\u1077\u1076\u1080\u1094\u1080\u1085\u1089\u1082\u1080\u1093\u32 \u1094\u1077\u1083\u1103\u1093\u32 \u1086\u1090\u1076\u1077\u1083\u1100\u1085\u1086\u32 \u1080\u1083\u1080\u32 \u1074\u32 \u1089\u1086\u1095\u1077\u1090\u1072\u1085\u1080\u1080\u32 \u1084\u1077\u1078\u1076\u1091\u32 \u1089\u1086\u1073\u1086\u1081,\u32 \u1072\u32 \u1090\u1072\u1082\u1078\u1077\u32 \u1074\u1084\u1077\u1089\u1090\u1077\u32 \u1089\u32 \u1076\u1088\u1091\u1075\u1080\u1084\u1080\u32 \u1087\u1088\u1080\u1085\u1072\u1076\u1083\u1077\u1078\u1085\u1086\u1089\u1090\u1103\u1084\u1080,\u32 \u1085\u1077\u1086\u1073\u1093\u1086\u1076\u1080\u1084\u1099\u1084\u1080\u32 \u1076\u1083\u1103\u32 \u1087\u1088\u1080\u1084\u1077\u1085\u1077\u1085\u1080\u1103\u32 \u1091\u1082\u1072\u1079\u1072\u1085\u1085\u1099\u1093\u32 \u1080\u1079\u1076\u1077\u1083\u1080\u1081\u32 \u1087\u1086\u32 \u1085\u1072\u1079\u1085\u1072\u1095\u1077\u1085\u1080\u1102,\u32 \u1074\u1082\u1083\u1102\u1095\u1072\u1103\u32 \u1089\u1087\u1077\u1094\u1080\u1072\u1083\u1100\u1085\u1086\u1077\u32 \u1087\u1088\u1086\u1075\u1088\u1072\u1084\u1084\u1085\u1086\u1077\u32 \u1086\u1073\u1077\u1089\u1087\u1077\u1095\u1077\u1085\u1080\u1077,\u32 \u1080\u32 \u1087\u1088\u1077\u1076\u1085\u1072\u1079\u1085\u1072\u1095\u1077\u1085\u1085\u1099\u1093\u32 \u1087\u1088\u1086\u1080\u1079\u1074\u1086\u1076\u1080\u1090\u1077\u1083\u1077\u1084\u32 \u1076\u1083\u1103\u32 \u1087\u1088\u1086\u1092\u1080\u1083\u1072\u1082\u1090\u1080\u1082\u1080,\u32 \u1076\u1080\u1072\u1075\u1085\u1086\u1089\u1090\u1080\u1082\u1080,\u32 \u1083\u1077\u1095\u1077\u1085\u1080\u1103\u32 \u1080\u32 \u1084\u1077\u1076\u1080\u1094\u1080\u1085\u1089\u1082\u1086\u1081\u32 \u1088\u1077\u1072\u1073\u1080\u1083\u1080\u1090\u1072\u1094\u1080\u1080\u32 \u1079\u1072\u1073\u1086\u1083\u1077\u1074\u1072\u1085\u1080\u1081,\u32 \u1084\u1086\u1085\u1080\u1090\u1086\u1088\u1080\u1085\u1075\u1072\u32 \u1089\u1086\u1089\u1090\u1086\u1103\u1085\u1080\u1103\u32 \u1086\u1088\u1075\u1072\u1085\u1080\u1079\u1084\u1072\u32 \u1095\u1077\u1083\u1086\u1074\u1077\u1082\u1072,\u32 \u1087\u1088\u1086\u1074\u1077\u1076\u1077\u1085\u1080\u1103\u32 \u1084\u1077\u1076\u1080\u1094\u1080\u1085\u1089\u1082\u1080\u1093\u32 \u1080\u1089\u1089\u1083\u1077\u1076\u1086\u1074\u1072\u1085\u1080\u1081,\u32 \u1074\u1086\u1089\u1089\u1090\u1072\u1085\u1086\u1074\u1083\u1077\u1085\u1080\u1103,\u32 \u1079\u1072\u1084\u1077\u1097\u1077\u1085\u1080\u1103,\u32 \u1080\u1079\u1084\u1077\u1085\u1077\u1085\u1080\u1103\u32 \u1072\u1085\u1072\u1090\u1086\u1084\u1080\u1095\u1077\u1089\u1082\u1086\u1081\u32 \u1089\u1090\u1088\u1091\u1082\u1090\u1091\u1088\u1099\u32 \u1080\u1083\u1080\u32 \u1092\u1080\u1079\u1080\u1086\u1083\u1086\u1075\u1080\u1095\u1077\u1089\u1082\u1080\u1093\u32 \u1092\u1091\u1085\u1082\u1094\u1080\u1081\u32 \u1086\u1088\u1075\u1072\u1085\u1080\u1079\u1084\u1072,\u32 \u1087\u1088\u1077\u1076\u1086\u1090\u1074\u1088\u1072\u1097\u1077\u1085\u1080\u1103\u32 \u1080\u1083\u1080\u32 \u1087\u1088\u1077\u1088\u1099\u1074\u1072\u1085\u1080\u1103\u32 \u1073\u1077\u1088\u1077\u1084\u1077\u1085\u1085\u1086\u1089\u1090\u1080,\u32 \u1092\u1091\u1085\u1082\u1094\u1080\u1086\u1085\u1072\u1083\u1100\u1085\u1086\u1077\u32 \u1085\u1072\u1079\u1085\u1072\u1095\u1077\u1085\u1080\u1077\u32 \u1082\u1086\u1090\u1086\u1088\u1099\u1093\u32 \u1085\u1077\u32 \u1088\u1077\u1072\u1083\u1080\u1079\u1091\u1077\u1090\u1089\u1103\u32 \u1087\u1091\u1090\u1077\u1084\u32 \u1092\u1072\u1088\u1084\u1072\u1082\u1086\u1083\u1086\u1075\u1080\u1095\u1077\u1089\u1082\u1086\u1075\u1086,\u32 \u1080\u1084\u1084\u1091\u1085\u1086\u1083\u1086\u1075\u1080\u1095\u1077\u1089\u1082\u1086\u1075\u1086,\u32 \u1075\u1077\u1085\u1077\u1090\u1080\u1095\u1077\u1089\u1082\u1086\u1075\u1086\u32 \u1080\u1083\u1080\u32 \u1084\u1077\u1090\u1072\u1073\u1086\u1083\u1080\u1095\u1077\u1089\u1082\u1086\u1075\u1086\u32 \u1074\u1086\u1079\u1076\u1077\u1081\u1089\u1090\u1074\u1080\u1103\u32 \u1085\u1072\u32 \u1086\u1088\u1075\u1072\u1085\u1080\u1079\u1084\u32 \u1095\u1077\u1083\u1086\u1074\u1077\u1082\u1072)\u32 \u1087\u1086\u1084\u1080\u1084\u1086\u32 \u1089\u1074\u1077\u1076\u1077\u1085\u1080\u1081,\u32 \u1091\u1082\u1072\u1079\u1072\u1085\u1085\u1099\u1093\u32 \u1074\u32 \u1087\u1091\u1085\u1082\u1090\u1072\u1093\u32 11\u32 \u1080\u32 12\u32 \u1085\u1072\u1089\u1090\u1086\u1103\u1097\u1080\u1093\u32 \u1055\u1088\u1072\u1074\u1080\u1083,\u32 \u1076\u1086\u1083\u1078\u1085\u1072\u32 \u1089\u1086\u1076\u1077\u1088\u1078\u1072\u1090\u1100\u32 \u1089\u1074\u1077\u1076\u1077\u1085\u1080\u1103\u32 \u1086\u32 \u1085\u1086\u1084\u1077\u1088\u1077\u32 \u1080\u32 \u1076\u1072\u1090\u1077\u32 \u1088\u1077\u1075\u1080\u1089\u1090\u1088\u1072\u1094\u1080\u1086\u1085\u1085\u1086\u1075\u1086\u32 \u1091\u1076\u1086\u1089\u1090\u1086\u1074\u1077\u1088\u1077\u1085\u1080\u1103\u32 \u1085\u1072\u32 \u1084\u1077\u1076\u1080\u1094\u1080\u1085\u1089\u1082\u1086\u1077\u32 \u1080\u1079\u1076\u1077\u1083\u1080\u1077,\u32 \u1074\u1099\u1076\u1072\u1085\u1085\u1086\u1075\u1086\u32 \u1060\u1077\u1076\u1077\u1088\u1072\u1083\u1100\u1085\u1086\u1081\u32 \u1089\u1083\u1091\u1078\u1073\u1086\u1081\u32 \u1087\u1086\u32 \u1085\u1072\u1076\u1079\u1086\u1088\u1091\u32 \u1074\u32 \u1089\u1092\u1077\u1088\u1